{
    "type": "package",
    "name": "mikaxyz/elm-gltf",
    "summary": "Import 3d assets from glTF (Graphics Library Transmission Format) file format",
    "license": "BSD-3-Clause",
    "version": "3.0.1",
    "exposed-modules": [
        "Gltf",
        "Gltf.Animation",
        "Gltf.Camera",
        "Gltf.Mesh",
        "Gltf.Material",
        "Gltf.Node",
        "Gltf.NodeIndex",
        "Gltf.Scene",
        "Gltf.Skin",
        "Gltf.Transform"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "NoRedInk/elm-json-decode-pipeline": "1.0.1 <= v < 2.0.0",
        "TSFoster/elm-bytes-extra": "1.3.0 <= v < 2.0.0",
        "chelovek0v/bbase64": "1.0.1 <= v < 2.0.0",
        "danfishgold/base64-bytes": "1.1.0 <= v < 2.0.0",
        "elm/bytes": "1.0.8 <= v < 2.0.0",
        "elm/core": "1.0.0 <= v < 2.0.0",
        "elm/http": "2.0.0 <= v < 3.0.0",
        "elm/json": "1.0.0 <= v < 2.0.0",
        "elm-explorations/linear-algebra": "1.0.3 <= v < 2.0.0",
        "elm-explorations/webgl": "1.1.3 <= v < 2.0.0",
        "nphollon/geo3d": "2.1.2 <= v < 3.0.0",
        "zwilias/elm-bytes-parser": "1.0.0 <= v < 2.0.0",
        "zwilias/elm-rosetree": "1.5.0 <= v < 2.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "2.0.0 <= v < 3.0.0"
    }
}
